{
  "rules": {
    "videos": {
      ".read": true,
      ".write": false
    },
    "quiz": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "answers": {
      ".read": "auth != null",
      ".write": "auth != null"
    },
    "result": {
      ".read": true,
      
      "$uid": {
        ".read": "$uid === auth.uid",

        ".write": "$uid === auth.uid"
      }
    }
  }
}
